Checks: >
  -*,
  bugprone-*,
  modernize-*,
  readability-*,
  performance-*,
  cppcoreguidelines-*,
  clang-analyzer-*,
  misc-*,
  -modernize-use-trailing-return-type,
  -readability-named-parameter
WarningsAsErrors: '*'
HeaderFilterRegex: '.*'
FormatStyle:     none
User:            ''
CheckOptions:
  - key:             modernize-use-nullptr.NullMacros
    value:           'NULL'
  - key:             modernize-use-override.IgnoreDestructors
    value:           '1'

